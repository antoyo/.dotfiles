search-engine aw https://wiki.archlinux.org/index.php?search={}
search-engine ar https://aur.archlinux.org/packages/?O=0&K={}
search-engine cr https://crates.io/search?q={}
search-engine rs https://doc.rust-lang.org/stable/std/?search={}
search-engine ia http://web.archive.org/web/*/{}
search-engine rd https://docs.rs/releases/search?query={}
search-engine mdn https://developer.mozilla.org/en-US/search?q={}
